{
  "name": "semantic-redux-form-fields",
  "version": "0.2.11",
  "description": "A set of form field components combining Semantic UI React and Redux Form",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run styleguide",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && codecov",
    "watch": "cross-env NODE_ENV=development webpack -w",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:production": "cross-env NODE_ENV=production webpack -p",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "prepublishOnly": "npm run build:production",
    "precommit": "lint-staged",
    "prepush": "npm run test"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "raf/polyfill",
      "./src/utils/setupTests.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.js",
      "!src/utils/**",
      "!src/styleguide/**",
      "!**/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariusespejo/semantic-redux-form-fields.git"
  },
  "keywords": [
    "semantic-ui",
    "redux-form",
    "react"
  ],
  "author": "Marius Espejo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusespejo/semantic-redux-form-fields/issues"
  },
  "homepage": "https://github.com/mariusespejo/semantic-redux-form-fields#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "jest-enzyme": "^6.0.0",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-styleguidist": "^6.4.0",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-mock-store": "^1.5.1",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.0",
    "webpack": "^3.10.0"
  },
  "peerDependency": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "semantic-ui-react": "^0.79.0"
  },
  "dependencies": {
    "moment": "^2.21.0",
    "react-datepicker": "^1.2.2"
  }
}
