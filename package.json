{
  "name": "semantic-redux-form-fields",
  "version": "0.3.6",
  "description": "A set of form field components combining Semantic UI React and Redux Form",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run styleguide",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && codecov",
    "watch": "webpack --mode 'development' --watch",
    "build:dev": "webpack --progress --display-error-details --mode 'development'",
    "build:production": "webpack",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "prepublishOnly": "npm run build:production",
    "precommit": "lint-staged",
    "prepush": "npm run test"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "raf/polyfill",
      "./src/utils/setupTests.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.js",
      "!src/utils/**",
      "!src/styleguide/**",
      "!**/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariusespejo/semantic-redux-form-fields.git"
  },
  "keywords": [
    "semantic-ui",
    "redux-form",
    "react"
  ],
  "author": "Marius Espejo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusespejo/semantic-redux-form-fields/issues"
  },
  "homepage": "https://github.com/mariusespejo/semantic-redux-form-fields#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "file-loader": "^2.0.0",
    "husky": "^1.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^6.1.2",
    "lint-staged": "^7.3.0",
    "moment": "^2.22.2",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-datepicker": "^1.6.0",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-styleguidist": "^7.3.8",
    "react-test-renderer": "^16.5.2",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-mock-store": "^1.5.3",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^0.82.5",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "peerDependency": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "semantic-ui-react": "^0.82.5",
    "redux-form": "^7.4.2",
    "react-datepicker": "^1.6.0",
    "moment": "^2.22.2"
  }
}
