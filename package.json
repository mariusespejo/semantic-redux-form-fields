{
  "name": "semantic-redux-form-fields",
  "version": "0.3.1",
  "description": "A set of form field components combining Semantic UI React and Redux Form",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run styleguide",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && codecov",
    "watch": "webpack --mode 'development' --watch",
    "build:dev": "webpack --progress --display-error-details --mode 'development'",
    "build:production": "webpack",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "prepublishOnly": "npm run build:production",
    "precommit": "lint-staged",
    "prepush": "npm run test"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "raf/polyfill",
      "./src/utils/setupTests.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.js",
      "!src/utils/**",
      "!src/styleguide/**",
      "!**/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariusespejo/semantic-redux-form-fields.git"
  },
  "keywords": [
    "semantic-ui",
    "redux-form",
    "react"
  ],
  "author": "Marius Espejo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusespejo/semantic-redux-form-fields/issues"
  },
  "homepage": "https://github.com/mariusespejo/semantic-redux-form-fields#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.1",
    "jest-enzyme": "^6.0.2",
    "lint-staged": "^7.2.0",
    "moment": "^2.22.2",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-styleguidist": "^7.1.0",
    "react-test-renderer": "^16.4.1",
    "react-datepicker": "^1.5.0",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-mock-store": "^1.5.3",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.1",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  },
  "peerDependency": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "semantic-ui-react": "^0.82.1",
    "redux-form": "^7.4.2",
    "react-datepicker": "^1.5.0",
    "moment": "^2.22.2"
  }
}
